{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/require-extension": "off",
    "react/prop-types": "off",
    "react/prefer-stateless-function": "off",
    "comma-dangle": "off",
    "no-underscore-dangle": "off"
  }
}
